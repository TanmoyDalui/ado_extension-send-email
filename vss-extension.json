{
    "manifestVersion": 1,
    "id": "dalui-tanmoy-sendemail",
    "version": "1.0.6",
    "name": "Send Email without SMPT config",
    "description": "The task can send email to Team members without SMTP configuration",
    "publisher": "tanmoydalui",
    "galleryFlags": [
            "Public"
    ],
    "tags": [
        "build",
        "email",
        "task"
    ],
    "categories": [
        "Azure Pipelines"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "img/logo.png"
    },
    "branding": {
        "color": "rgb(6, 109, 122)",
        "theme": "dark"
    },
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "license-terms.md"
        }
    },
    "contributions": [
        {
            "id": "custom-build-release-task-sendemail",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildandreleasetask",
                "description": "The task can send email to Team members without SMTP configuration",
                "catalogIconUrl": "img/logo.png",
                "previewImageUrl": "img/logo.png"
            }
        }
    ],
    "files": [
        {
            "path": "buildandreleasetask", "addressable": true
        },
        {
            "path": "overview.md", "addressable": true
        },
        {
            "path": "license-terms.md", "addressable": true
        },
        {
            "path": "img", "addressable": true
        }
    ]
}